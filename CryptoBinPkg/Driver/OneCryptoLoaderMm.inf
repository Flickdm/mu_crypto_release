  PeCoffGetEntryPointLib
  MemoryAllocationLib
  CacheMaintenanceLib
  MmServicesTableLib
  BaseMemoryLib
  PeCoffLib
  DebugLib
  SafeIntLib
  #PrePiLib
  HobLib
  RngLib
  FvLib

[Protocols]
  gOneCryptoMmProtocolGuid         ## PRODUCES
  gOneCryptoPrivateProtocolGuid    ## CONSUMES
  gEfiRngProtocolGuid                 ## CONSUMES

[Depex]
  gOneCryptoPrivateProtocolGuid 
  # AND gEfiMemoryAttributeProtocolGuid
